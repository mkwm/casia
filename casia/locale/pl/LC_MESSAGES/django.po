#: contrib/serializers.py:6
msgid "yes"
msgstr "tak"

#: contrib/serializers.py:7
msgid "no"
msgstr "nie"

#: core/serializers.py:37
msgid "true"
msgstr "prawda"

#: core/serializers.py:38
msgid "false"
msgstr "fałsz"

#: webapp/views.py:55
msgid "You have been logged out successfully."
msgstr "Zostałeś wylogowany pomyślnie."

#: webapp/views.py:57
msgid "You have to be logged in to log out. Please log in to log out."
msgstr ""
"Musisz być zalogowany aby się wylogować. Zaloguj się aby się wylogować."

#: webapp/templates/webapp/base.html:31
msgid "Admin site"
msgstr "Panel administracyjny"

#: webapp/templates/webapp/base.html:33
msgid "Change password"
msgstr "Zmień hasło"

#: webapp/templates/webapp/base.html:35
msgid "Log out"
msgstr "Wyloguj"

#: webapp/templates/webapp/confirm.html:4
msgid "Confirmation"
msgstr "Potwierdzenie"

#: webapp/templates/webapp/confirm.html:6
#, python-format
msgid "Authentication to <strong>%(name)s</strong> requires your consent"
msgstr "Uwierzytelnienie do <strong>%(name)s</strong> wymaga twojej zgody"

#: webapp/templates/webapp/confirm.html:8
#, python-format
msgid ""
"Authentication to service at <strong>%(url)s</strong> requires your consent"
msgstr ""
"Uwierzytelnienie do usługi pod adresem <strong>%(url)s</strong> wymaga "
"twojej zgody"

#: webapp/templates/webapp/confirm.html:12
#, python-format
msgid "Service is operated by <strong>%(owner)s</strong> (trusted provider)"
msgstr ""
"Usługa jest obsługiwana przez <strong>%(owner)s</strong> (zaufany dostawca)"

#: webapp/templates/webapp/confirm.html:14
#, python-format
msgid "Service is operated by <strong>%(owner)s</strong>"
msgstr "Usługa jest obsługiwana przez <strong>%(owner)s</strong>"

#: webapp/templates/webapp/confirm.html:17
msgid ""
"Service is operated by unknown provider &ndash; <strong>be careful!</strong>"
msgstr ""
"Usługa jest obsługiwana przez nieznanego dostawcę &ndash; <strong>zachowaj "
"ostrożność!</strong>"

#: webapp/templates/webapp/confirm.html:21
msgid "Service will receive details from user profile"
msgstr "Usługa otrzyma dane z profilu użytkownika"

#: webapp/templates/webapp/confirm.html:28
msgid "Service will not receive details from user profile"
msgstr "Usługa nie otrzyma danych z profilu użytkownika"

#: webapp/templates/webapp/confirm.html:34
msgid "Continue"
msgstr "Kontynuuj"

#: webapp/templates/webapp/confirm.html:35
msgid "Abort"
msgstr "Przerwij"

#: webapp/templates/webapp/logged_in.html:4
msgid "Logged in"
msgstr "Zalogowany"

#: webapp/templates/webapp/logged_in.html:5
msgid "You are logged into Casia"
msgstr "Jesteś zalogowany w Casia"

#: webapp/templates/webapp/logged_in.html:6
msgid "Authentication to services would be performed automatically"
msgstr "Uwierzytelnianie do usług będzie wykonywane automatyczne"

#: webapp/templates/webapp/login_form.html:30
msgid "Log in"
msgstr "Zaloguj"

#: webapp/templates/webapp/login_form.html:33
msgid "Security warning"
msgstr "Ostrzeżenie bezpieczeństwa"

#: webapp/templates/webapp/login_form.html:34
msgid ""
"Always check the address bar when logging in to Casia. Do not trust other "
"sites with your password!"
msgstr ""
"Zawsze sprawdzaj pasek adresu przed zalogowaniem się do Casia. Nie podawaj "
"swojego hasła na innych stronach!"

#: webapp/templates/webapp/relogin.html:4
msgid "Reauthentication"
msgstr "Ponowne uwierzytelnienie"

#: webapp/templates/webapp/relogin.html:5
msgid "Renew your credentials for security reasons"
msgstr "Ze względów bezpieczeństwa zaloguj się ponownie"

#: webapp/templates/webapp/unknown.html:4
msgid "Unknown service"
msgstr "Nieznana usługa"

#: webapp/templates/webapp/unknown.html:5
msgid "Authentication aborted due to security reasons"
msgstr "Uwierzytelnianie przerwane ze względów bezpieczeństwa"

#: webapp/templates/webapp/unknown.html:6
msgid "Service is not known to Casia"
msgstr "Usługa nie jest znana w Casia"
